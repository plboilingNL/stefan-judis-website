#!/bin/bash

if [ "$WEBHOOK_TITLE" = CLEAR_CUSTOM_CACHE ]; then
  echo "Clear webhook detected - skipping unpacking archive"
else
  mkdir ./dist

  wget -qO- -O ./dist/tmp.zip https://www.stefanjudis.com/archive.zip && unzip ./dist/tmp.zip -d ./dist && rm dist/tmp.zip
  mv  dist/dist/* dist
  rm -r dist/dist
fi
